<h2>One specific user</h2>

<h3><%=@user.username%></h3>
<p>Date Registerd: <%=@user.created_at%></p>


<h1>All posts</h1>
<ol>
  <%@posts.each do |post|%>
  <li><%=post.title%></li>
  <%=link_to 'Go to post', post%>
  <%=link_to 'Delete this post', post, {method: 'delete'}%>
  <%=link_to 'Edit this post', edit_post_path(post)%>
  <%end%>
</ol>
